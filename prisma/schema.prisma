// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Leaderboard {
  id       String   @id @default(uuid())
  username String
  points   Int      @default(0)
  playedAt DateTime @default(now())
}
